{
  "name": "vipnytt/robotstxtparser",
  "description": "Robots.txt parser class, according to Google, Yandex, W3C and The Web Robots Pages specifications.",
  "keywords": [
    "robots.txt",
    "parser",
    "Google",
    "Yandex",
    "W3C",
    "The Web Robots Pages",
    "Robot",
    "Spider",
    "Web-Crawler"
  ],
  "homepage": "https://github.com/VIPnytt/RobotsTxtParser",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.4.0",
    "vipnytt/useragentparser": "0.2.*",
    "guzzlehttp/guzzle": "6.*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.0",
    "codeclimate/php-test-reporter": ">=0.2.0"
  },
  "authors": [
    {
      "name": "VIP nytt",
      "email": "vipnytt@gmail.com",
      "role": "creator"
    },
    {
      "name": "Jan-Petter Gundersen",
      "email": "europe.jpg@gmail.com",
      "role": "developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "vipnytt\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "vipnytt\\RobotsTxtParser\\Tests\\": "tests/"
    }
  }
}
